{
  "name": "@blaxel/mcp-gateway",
  "version": "0.0.4",
  "description": "MCP Gateway for Blaxel",
  "license": "MIT",
  "author": "Blaxel, Inc. (https://blaxel.ai)",
  "homepage": "https://blaxel.ai",
  "bugs": "https://github.com/blaxel-ai/mcp-gateway/issues",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/blaxel-ai/mcp-gateway.git"
  },
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prebuild": "pnpm typecheck",
    "prepublishOnly": "pnpm build",
    "start": "node dist/index.js --url wss://run.blaxel.ai/main/functions/blaxel-search"
  },
  "bin": {
    "mcp-gateway": "./dist/index.mjs"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@blaxel/core": "^0.2.31",
    "@modelcontextprotocol/sdk": "^1.17.4"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^20.10.0",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2"
  }
}
